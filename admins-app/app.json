{
    "expo": {
        "name": "DCN Main",
        "slug": "online-iptv-hub-main",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "scheme": "onlineiptvhubmain ",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.miteshhq.onlineiptvhubmain"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.miteshhq.onlineiptvhubmain",
            "versionCode": 1
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/icon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/icon.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {},
            "eas": {
                "projectId": "41cbb423-a309-4805-bd08-89bfe06fc300"
            }
        }
    }
}